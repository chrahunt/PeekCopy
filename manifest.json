{
  "manifest_version": 2,
  "name": "PeekCopier",
  "version": "1.1",
  "description": "This extension is supposed to text the capability of a chrome extension",
  "background": {
    "scripts": ["background.js"]
  },
  "page_action" :
  {
    "default_icon": "images/icon38.png",
    "default_title": "TEST"
  },
  "content_scripts": [
    {
      "matches":["http://www.duolingo.com/skill/*"],
      "js": ["test.js"]
    }
  ],
  "permissions": [
    "tabs",
    "storage"
  ]
}